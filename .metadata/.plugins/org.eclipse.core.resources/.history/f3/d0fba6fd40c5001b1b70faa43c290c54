import kodlamaio.hrms.entities.concretes.SchoolInfo;
import kodlamaio.hrms.entities.concretes.Technology;
import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
public class ResumeShowDto {
	
	private String photo;
	private String githubLink;
	private String linkedinLink;
	private String coverLatter;
	private Date createdDate;
	private Date lastUpdateDate;
	private int candidateId;
	private String candidateFirstName;
	private String candidateLastName;
	private String candidateBirthDate;
	private List<BusinessExperience> businessExperiences;
	private List<ForeignLanguage> foreignLanguages;
	private List<SchoolInfo> schoolInfos;
	private List<Technology> technologies;
}
